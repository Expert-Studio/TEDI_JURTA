  inherited SZURES: TADOQuery
    Parameters = <
      item
        Name = 'BERLO_ID'
        DataType = ftString
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      'Select a.*, b.berles_kezdet_datuma, b.berles_vege_datuma,'
      
        '  (Select x.szemely_teljes_neve From szemely x Where x.szemely_i' +
        'd=a.szemely_id) as szemely,'
      
        '  (Select x.anyja_neve From szemely x Where x.szemely_id=a.szeme' +
        'ly_id) as anyja,'
      
        '  (Select x.jurta_kod From szemely x Where x.szemely_id=a.szemel' +
        'y_id) as kod,'
      
        '  (Select x.szuletesi_datum From szemely x Where x.szemely_id=a.' +
        'szemely_id) as szdat'
      'From berlok a, berlo_kapcsolat b'
      'Where a.berlo_id=b.berlo_id and a.berlo_id = :BERLO_ID')
    Left = 584
    Top = 384
  end
